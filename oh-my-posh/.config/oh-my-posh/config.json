{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "catppuccin-bg": "#1e1e2e",
    "catppuccin-surface": "#313244", 
    "catppuccin-surface-bright": "#45475a",
    "catppuccin-blue": "#cba6f7",
    "catppuccin-green": "#a6e3a1",
    "catppuccin-yellow": "#fab387",
    "catppuccin-pink": "#f5c2e7",
    "catppuccin-teal": "#94e2d5",
    "catppuccin-text": "#cdd6f4",
    "catppuccin-white": "#f5f5f5"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:catppuccin-surface",
          "foreground": "p:catppuccin-white",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": "{{ if .WSL }}WSL at{{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:catppuccin-surface-bright",
          "foreground": "p:catppuccin-white",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "p:catppuccin-blue",
          "foreground": "p:catppuccin-bg",
          "properties": {
            "style": "agnoster_short",
            "folder_icon": "…",
            "home_icon": "~"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:catppuccin-green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:catppuccin-yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:catppuccin-pink{{ end }}",
            "{{ if gt .Ahead 0 }}p:catppuccin-pink{{ end }}",
            "{{ if gt .Behind 0 }}p:catppuccin-pink{{ end }}"
          ],
          "foreground": "p:catppuccin-bg",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "branch_icon": "\uE0A0 ",
            "branch_identical_icon": "\u25CF"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:catppuccin-surface-bright",
          "foreground": "p:catppuccin-white",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }} ",
          "properties": {
            "discharging_icon": "\uE234 ",
            "charging_icon": "\uF1E6 ",
            "charged_icon": "\uE234 "
          },
          "type": "battery"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:catppuccin-green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "❯ ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:catppuccin-blue",
    "template": "❯ "
  },
  "final_space": false,
  "disable_notice": true,
  "version": 3
}
